<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id="dollars_quote_product_product_form_view" model="ir.ui.view">
      <field name="name">dollars_quote.product.product.form</field>
      <field name="model">product.product</field>
      <field name="inherit_id" ref="product.product_normal_form_view"/>
      <field name="arch" type="xml">
        <div name="standard_price_uom" position="after">
          <field name="usd_com"/>
          <field name="workforce"/>
          <field name="gif"/>
          <field name="usd_currency_id" invisible="1"/>
        </div>
      </field>
    </record>
  </data>
</odoo>